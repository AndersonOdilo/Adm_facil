 $("#unidade_modal").html("<%= escape_javascript(render partial: 'form_modal') %>");
 $('#nova_unidade').modal('show');
 $(function() {
  $('#add_unidade').on("click", function() {
    return $('#new_unidade').submit();
  });
  return $('#new_unidade').bind("ajax:success", function(event, data, status, xhr) {
    $('#produto_unidade').append($("<option/>").attr("value", data.id).text(data.descricao).attr("selected", "selected"));
    $('#nova_unidade').modal('hide');
    return $('#new_unidade')[0].reset();
  });
});

